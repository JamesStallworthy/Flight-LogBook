{
  "name": "flight-logbook",
  "version": "1.0.0",
  "description": "A log book to record flight time",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . flight-logbook --overwrite --asar --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=JS --version-string.FileDescription=FlightLogBook --version-string.ProductName=\"Flight-LogBook\""
  },
  "author": "James Stallworthy",
  "license": "MIT",
  "dependencies": {
    "csv-parser": "^2.3.3",
    "csv-writer": "^1.6.0",
    "electron": "^10.1.0"
  },
  "devDependencies": {
    "electron-packager": "^15.1.0"
  }
}
